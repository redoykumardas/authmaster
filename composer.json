{
    "name": "redoy/authmaster",
    "description": "AuthMaster - A production-ready, service-driven authentication package for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Redoy",
            "email": "redoykumardas.office@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0 || ^8.1",
        "illuminate/support": "^8.0|^9.0|^10.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Redoy\\AuthMaster\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Redoy\\AuthMaster\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Redoy\\AuthMaster\\Providers\\AuthMasterServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^10.8"
    }
}